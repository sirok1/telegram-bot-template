{
  "name": "telegram-bot-template",
  "version": "1.0.0-snapshot",
  "description": "",
  "main": "build/app.js",
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/app.ts",
    "ext": "ts, js"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node build/app.js",
    "build": "tsc && resolve-tspaths",
    "build:doc": "pnpm typedoc src/app.ts",
    "dev": "cross-env NODE_ENV=development nodemon",
    "force-start": "cross-env NODE_ENV=production tsc && node build/app.js",
    "lint": "eslint . --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format-db-scheme": "prisma format",
    "prisma-dev-migration": "prisma migrate dev",
    "prisma-db-push": "prisma db push",
    "prisma studio": "cross-env BROWSER=none pnpm prisma studio",
    "generate-prisma-client": "prisma generate"
  },
  "keywords": [],
  "author": "GPL-3.0-or-later",
  "license": "ISC",
  "devDependencies": {
    "@microsoft/tsdoc": "^0.14.2",
    "@types/amqplib": "^0.10.4",
    "@types/node": "^20.11.0",
    "cross-env": "^7.0.3",
    "node-env": "^0.1.6",
    "nodemon": "^3.0.2",
    "prisma": "^5.8.1",
    "resolve-tspaths": "^0.8.17",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typedoc": "^0.25.7",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@prisma/client": "5.8.1",
    "amqplib": "^0.10.3",
    "dotenv": "^16.3.1",
    "redis": "^4.6.12",
    "telegraf": "^4.15.3",
    "winston": "^3.11.0",
    "winston-loki": "^6.0.8"
  },
  "engines": {
    "node": ">=20.10.0"
  }
}
